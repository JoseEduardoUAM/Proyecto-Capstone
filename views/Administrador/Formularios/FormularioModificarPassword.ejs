
<form class="container bg-white border rounded p-3 mb-4" id="FormularioPassword">
    <!-- Titulo del formulario -->
    <div class="row">
        <div class="col">
            <h5> Modificar Contraseña </h5>
        </div>
    </div>
    
    <!-- Inputs del formulario -->
    <div class="row mb-2 mt-2">
        <div class="col-12">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="1" id="ModificarPassword">
                <label class="form-check-label" for="ModificarPassword"> Modificar Contraseña </label>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-6">
            <label for="Password" class="form-label">Nueva Contraseña:</label>
            <input type="password" class="form-control" id="Password" disabled>

        </div>
    </div>
</form>